{
  "manifest_version": 3,
  "name": "Dark Pattern Finder",
  "version": "1.0",
  "description": "This is my Chrome extension using React.",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["tabs", "activeTab", "notifications", "scripting"],
  "host_permissions": [
      "https://*/*",
      "http://*/*"
  ],
  "background": {
    "service_worker": "background.js"
  }
}
